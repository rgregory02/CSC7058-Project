{
  "name": "Pet",
  "description": "A person's pet",
  "input": "select",
  "source": {
    "kind": "type_biographies",
    "type": "pet",
    "path": "pet",

    "allow_children": true
  },

  
  "link_biography": {
    "type": "pet",
    "mode": "child_or_parent",
    "lookup": "id",
    "path": "pet"
  },
  "ui": {
    "allow_confidence": true,
    "default_confidence": 100,
    "display_template": "Has pet: {{option}}"
  }
}