{
    "label": "Hospitals in Wales",
    "description": "",
    "order": 999,
    "ingest": {
        "kind": "api",
        "url": "https://query.wikidata.org/sparql",
        "method": "GET",
        "headers_env": "",
        "query_json": {
            "format": "json",
            "query": "SELECT ?hospital ?hospitalLabel WHERE { ?hospital wdt:P31 wd:Q16917; wdt:P17 wd:Q26. SERVICE wikibase:label { bd:serviceParam wikibase:language 'en'. } } LIMIT 15"
        },
        "array_path": "results.bindings",
        "field_id": "hospital.value",
        "field_display": "hospitalLabel.value",
        "field_desc": "Labels for hospitals in Wales",
        "field_image": "",
        "max_items": 15,
        "postprocess": {
            "strip_wikidata_uri": true
        }
    },
    "allow_children": false
}